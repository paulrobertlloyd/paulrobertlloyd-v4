{
  "private": true,
  "name": "paulrobertlloyd.com",
  "version": "4.0.0",
  "description": "The personal website of Paul Robert Lloyd",
  "keywords": [
    "indieweb",
    "personal-website"
  ],
  "author": {
    "name": "Paul Robert Lloyd",
    "email": "reply@paulrobertlloyd.com",
    "url": "https://paulrobertlloyd.com"
  },
  "homepage": "https://github.com/paulrobertlloyd/paulrobertlloyd-v4#readme",
  "repository": "github:paulrobertlloyd/paulrobertlloyd-v4",
  "bugs": "https://github.com/paulrobertlloyd/paulrobertlloyd-v4/issues",
  "scripts": {
    "build": "eleventy --quiet",
    "start": "eleventy --incremental --serve",
    "dev": "node --env-file .env --run start",
    "lint:format": "prettier . --check",
    "lint:format:fix": "prettier . --write",
    "lint:scripts": "eslint '**/*.js'",
    "lint:scripts:fix": "eslint '**/*.js' --fix",
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:styles:fix": "stylelint 'src/**/*.css'",
    "lint": "npm run lint:format && npm run lint:scripts && npm run lint:styles",
    "lint:fix": "npm run lint:format:fix && npm run lint:scripts:fix && npm run lint:styles:fix",
    "debug": "DEBUG=* npx eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "3.0.0-alpha.17",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^4.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11tyrocks/eleventy-plugin-lightningcss": "^1.1.1",
    "@paulrobertlloyd/markdown-it-rules": "paulrobertlloyd/markdown-it-rules",
    "geolib": "^3.3.1",
    "ical-js-parser": "^0.7.4",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.0.0",
    "markdown-it-attribution": "^0.1.4",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-handle": "^0.1.0",
    "markdown-it-image-figures": "^2.0.0",
    "mime": "^4.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-webmentions": "^1.1.1",
    "newbase60": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^48.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "prettier": "^3.0.1",
    "stylelint": "^16.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.0"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "type": "module",
  "browserslist": [
    ">= 5%"
  ]
}
