{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'footer'
    date: date
  -%}
  {%- render 'header'
    title: title
    summary: false
    url: url
    url_class: 'bookmark-of'
    vocab: vocab
  -%}
  {%- render 'content' with content -%}
  {%- render 'categories' with category -%}
  {%- render 'section-remarks' with webmentions
    id: 'webmentions'
    title: 'Webmentions'
  -%}
{%- endblock -%}
