{%- layout 'default' -%}
{%- block 'main' -%}
  {%- render 'header'
    title: title
    summary: summary
  -%}
  {%- assign items = collections.project | includes: 'data.category',title | reverse -%}
  {%- render 'items' with items
    component: 'card'
    gridSize: '12rem'
    itemImagePreset: 'square'
  -%}
{%- endblock -%}
