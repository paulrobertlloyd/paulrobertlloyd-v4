{
  "categories": {
    "url": "https://paulrobertlloyd.com/categories/index.json"
  },
  "syndicate-to": [{
    "uid": "https://twitter.com/paulrobertlloyd/",
    "name": "@paulrobertlloyd on Twitter"
  }, {
    "uid": "https://mastodon.social/@paulrobertlloyd",
    "name": "@paulrobertlloyd on Mastodon"
  }, {
    "uid": "https://micro.blog/paulrobertlloyd",
    "name": "@paulrobertlloyd on Micro.blog"
  }],
  "post-types": {
    "bookmark": {
      "post": {
        "path": "src/content/bookmarks/{{ published | date('yyyy-MM-dd') }}-{{ slug }}.md",
        "url": "bookmarks/{{ published | date('yyyy/MM') }}/{{ slug }}"
      }
    },
    "note": {
      "post": {
        "path": "src/content/notes/{{ published | date('X') }}.md",
        "url": "notes/{{ published | date('X') }}"
      }
    },
    "photo": {
      "post": {
        "path": "src/content/photos/{{ published | date('X') }}.md",
        "url": "photos/{{ published | date('X') }}"
      },
      "media": {
        "path": "src/images/photos/{{ filedate | date('X') }}.{{ fileext }}",
        "url": "images/photos/{{ filedate | date('X') }}.{{ fileext }}"
      }
    },
    "reply": {
      "post": {
        "path": "src/content/replies/{{ published | date('X') }}.md",
        "url": "replies/{{ published | date('X') }}"
      }
    }
  },
  "slug-separator": "_"
}
